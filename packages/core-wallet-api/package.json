{
    "name": "@arkecosystem/core-wallet-api",
    "version": "3.0.0-next.0",
    "description": "Wallet API for ARK Core",
    "license": "MIT",
    "contributors": [
        "Brian Faust <brian@ark.io>",
        "Alex Barnsley <alex@ark.io>"
    ],
    "files": [
